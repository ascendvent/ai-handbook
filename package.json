    "CLAUDE_GLOBAL.md",
    "agents/",
    "templates/",
    "scripts/",
    "README.md",
    "CHANGELOG.md",
    "VERSION.md",
    "LICENSE",
    "CONTRIBUTING.md"
  ],
  "scripts": {
    "test": "node scripts/validator/validate-policy.cjs",
    "validate": "npm run test && echo 'âœ… All components validated'",
